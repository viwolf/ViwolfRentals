<!DOCTYPE html>
<html lang="en">
<head>
    <title>Viwolf - Login</title>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <!--===============================================================================================-->
    <link rel="icon" type="image/png" href="~/Areas/Seguridad/Content/images/icons/favicon.ico" />
    <!--===============================================================================================-->
    <link rel="stylesheet" type="text/css" href="~/Areas/Seguridad/Content/Vendor/bootstrap/css/bootstrap.min.css">
    <link href="~/Areas/Seguridad/Content/vendor/bootstrap/css/bootstrap.min.css" rel="stylesheet" />
    <!--===============================================================================================-->
    <link rel="stylesheet" type="text/css" href="~/Areas/Seguridad/Content/fonts/font-awesome-4.7.0/css/font-awesome.min.css">
    <!--===============================================================================================-->
    <link rel="stylesheet" type="text/css" href="~/Areas/Seguridad/Content/Vendor/animate/animate.css">
    <!--===============================================================================================-->
    <link rel="stylesheet" type="text/css" href="~/Areas/Seguridad/Content/Vendor/css-hamburgers/hamburgers.min.css">
    <!--===============================================================================================-->
    <link rel="stylesheet" type="text/css" href="~/Areas/Seguridad/Content/Vendor/select2/select2.min.css">
    <!--===============================================================================================-->
    <link href="~/Content/Plugins/bootstrap-dialog/css/bootstrap-dialog.css" rel="stylesheet" />
    <link href="~/Areas/Seguridad/Content/css/util.css" rel="stylesheet" />
    <link href="~/Areas/Seguridad/Content/css/main.css" rel="stylesheet" />
    <!--===============================================================================================-->
</head>
<body>

    <div class="limiter">
        <div class="container-login100">
            <div class="wrap-login100">
                <div class="login100-pic js-tilt" data-tilt>
                    <img src="~/Areas/Seguridad/Content/images/Logo.png" alt="IMG">
                </div>

                <form class="login100-form validate-form">
                    <span class="login100-form-title">
                        Member Login
                    </span>

                    <div class="wrap-input100 validate-input" data-validate="Valid email is required: ex@abc.xyz">
                        <input id="txtUsuario" class="input100" type="text" name="email" placeholder="Usuario">
                        <span class="focus-input100"></span>
                        <span class="symbol-input100">
                            <i class="fa fa-envelope" aria-hidden="true"></i>
                        </span>
                    </div>

                    <div class="wrap-input100 validate-input" data-validate="Password is required">
                        <input id="txtPassword" class="input100" type="password" name="pass" placeholder="Password">
                        <span class="focus-input100"></span>
                        <span class="symbol-input100">
                            <i class="fa fa-lock" aria-hidden="true"></i>
                        </span>
                    </div>

                    <div class="container-login100-form-btn">
                        <button id="btnEntrar" class="login100-form-btn">
                            Entrar
                        </button>
                    </div>

                    <div class="text-center p-t-12">
                        <span class="txt1">
                            Olvidó
                        </span>
                        <a class="txt2" href="#">
                            Usuario / Password?
                        </a>
                    </div>

                    @*<div class="text-center p-t-136">
                            <a class="txt2" href="#">
                                Create your Account
                                <i class="fa fa-long-arrow-right m-l-5" aria-hidden="true"></i>
                            </a>
                        </div>*@
                </form>
            </div>
        </div>
    </div>




    <!--===============================================================================================-->
    <script src="~/Areas/Seguridad/Content/Vendor/jquery/jquery-3.2.1.min.js"></script>
    <!--===============================================================================================-->
    <script src="~/Areas/Seguridad/Content/Vendor/bootstrap/js/popper.js"></script>
    <script src="~/Areas/Seguridad/Content/Vendor/bootstrap/js/bootstrap.min.js"></script>
    <!--===============================================================================================-->
    <script src="~/Areas/Seguridad/Content/Vendor/select2/select2.min.js"></script>
    <!--===============================================================================================-->
    <script src="~/Areas/Seguridad/Content/Vendor/tilt/tilt.jquery.min.js"></script>
    <script>
        $('.js-tilt').tilt({
            scale: 1.1
        })
    </script>
    <!--===============================================================================================-->
    <script src="~/Content/Plugins/ajaxloader/ajaxloader.js"></script>
    <script src="~/Content/Plugins/bootstrap/js/bootstrap.min.js"></script>
    <script src="~/Content/Plugins/bootstrap-dialog/js/bootstrap-dialog.js"></script>
    <script src="~/Areas/Seguridad/Content/js/src/msjApp.js"></script>
    <script src="~/Areas/Seguridad/Content/js/src/app.js"></script>
    <script src="~/Areas/Seguridad/Content/js/src/Login/Login.js"></script>

</body>
</html>