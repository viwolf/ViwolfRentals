<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta http-equiv="X-UA-Compatible" content="IE=edge" />
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no" />
    <meta name="description" content="" />
    <meta name="author" content="" />
    <title>@ViewBag.Title - Viwolf</title>
    @Styles.Render("~/bundles/Estilo/ViwolfRentals")

</head>

<body class="sb-nav-fixed">

    <nav class="sb-topnav navbar navbar-expand navbar-dark bg-dark">
        <a class="navbar-brand"> Viwolf System</a><button class="btn btn-link btn-sm order-1 order-lg-0" id="sidebarToggle" href="#">
            <i class="fas fa-bars"></i>
        </button><!-- Navbar Search-->
        <form class="d-none d-md-inline-block form-inline ml-auto mr-0 mr-md-3 my-2 my-md-0">
            <div class="input-group">
                <input class="form-control" type="text" placeholder="Search for..." aria-label="Search" aria-describedby="basic-addon2" />
                <div class="input-group-append">
                    <button class="btn btn-primary" type="button"><i class="fas fa-search"></i></button>
                </div>
            </div>
        </form>
        <!-- Navbar-->
        <ul class="navbar-nav ml-auto ml-md-0">
            <li class="nav-item dropdown">
                <a class="nav-link dropdown-toggle" id="userDropdown" href="#" role="button" data-toggle="dropdown" aria-haspopup="true" aria-expanded="false"><i class="fas fa-user fa-fw"></i></a>
                <div class="dropdown-menu dropdown-menu-right" aria-labelledby="userDropdown">
                    @*<a class="dropdown-item" href="#">Settings</a>
                        <a class="dropdown-item" href="#">Activity Log</a>*@
                    <div class="dropdown-divider"></div>
                    <a class="dropdown-item" href="@Url.Action("Index","Login", new { area = "Seguridad" })">Logout</a>
                </div>
            </li>
        </ul>
    </nav>
    <div id="layoutSidenav">
        <div id="layoutSidenav_nav">
            <nav class="sb-sidenav accordion sb-sidenav-dark" id="sidenavAccordion">
                <div class="sb-sidenav-menu">
                    <div class="nav">
                        <div class="sb-sidenav-menu-heading">Sistemas</div>
                        <a class="nav-link collapsed" id="layoutWiwolfRentals" href="#" data-toggle="collapse" data-target="#collapseViwolfRentals" aria-expanded="false" aria-controls="collapseViwolfRentals" data-parent="#sidenavAccordion">
                            <div class="sb-nav-link-icon"><i class="fa fa-motorcycle"></i></div>
                            Viwolf Rentals
                            <div class="sb-sidenav-collapse-arrow"><i class="fas fa-angle-down"></i></div>
                        </a>
                        <div class="collapse" id="collapseViwolfRentals" aria-labelledby="headingTwo" data-parent="#layoutWiwolfRentals">
                            <nav class="sb-sidenav-menu-nested nav accordion" id="sidenavAccordionReservaciones">
                                <a class="nav-link" href="#">Dashboard</a>
                                <a class="nav-link collapsed" href="#" data-toggle="collapse" data-target="#pagesCollapseReservaciones" aria-expanded="false" aria-controls="pagesCollapseReservaciones">
                                    Reservaciones
                                    <div class="sb-sidenav-collapse-arrow"><i class="fas fa-angle-down"></i></div>
                                </a>
                                <div class="collapse" id="pagesCollapseReservaciones" aria-labelledby="headingOne" data-parent="#sidenavAccordionReservaciones">
                                    <nav class="sb-sidenav-menu-nested nav">
                                        <a class="nav-link" href="@Url.Action("Mantenimiento","Reservaciones", new { usuario = ViewBag.Usuario, idUsuario = ViewBag.IdUsuario } )">Mantenimiento</a>
                                        <a class="nav-link" href="@Url.Action("Index","Reservaciones", new { usuario = ViewBag.Usuario, idUsuario = ViewBag.IdUsuario } )">Creación</a>
                                    </nav>
                                </div>
                                <a class="nav-link collapsed" href="#" data-toggle="collapse" data-target="#pagesCollapseContratos" aria-expanded="false" aria-controls="pagesCollapseContratos">
                                    Contratos
                                    <div class="sb-sidenav-collapse-arrow"><i class="fas fa-angle-down"></i></div>
                                </a>
                                <div class="collapse" id="pagesCollapseContratos" aria-labelledby="headingOne" data-parent="#layoutWiwolfRentals">
                                    <nav class="sb-sidenav-menu-nested nav">
                                        <a class="nav-link" href="#">Mantenimiento</a>
                                        <a class="nav-link" href="#">Creación</a>
                                    </nav>
                                </div>
                                <a class="nav-link collapsed" href="#" data-toggle="collapse" data-target="#pagesCollapseError" aria-expanded="false" aria-controls="pagesCollapseError">
                                    Talleres
                                    <div class="sb-sidenav-collapse-arrow"><i class="fas fa-angle-down"></i></div>
                                </a>
                                <a class="nav-link collapsed" href="#" data-toggle="collapse" data-target="#pagesCollapseInventario" aria-expanded="false" aria-controls="pagesCollapseInventario">
                                    Inventarios
                                    <div class="sb-sidenav-collapse-arrow"><i class="fas fa-angle-down"></i></div>
                                </a>
                                <div class="collapse" id="pagesCollapseInventario" aria-labelledby="headingOne" data-parent="#layoutWiwolfRentals">
                                    <nav class="sb-sidenav-menu-nested nav">
                                        <a class="nav-link" href="@Url.Action("Index","Vehiculos", new { usuario = ViewBag.Usuario, idUsuario = ViewBag.IdUsuario } )">Ver Unidades</a>
                                        <a class="nav-link" href="@Url.Action("CreacionVehiculos","Vehiculos", new { usuario = ViewBag.Usuario, idUsuario = ViewBag.IdUsuario } )">Creación de unidades</a>
                                    </nav>
                                </div>
                            </nav>
                        </div>
                    </div>
                </div>
                <div class="sb-sidenav-footer">
                    <div class="small">Logged in as:</div>
                    @ViewBag.Usuario
                </div>
            </nav>
        </div>
        <div id="layoutSidenav_content">
            <main>
                @RenderBody()
            </main>
            <footer class="py-4 bg-light mt-auto">
                <div class="container-fluid">
                    <div class="d-flex align-items-center justify-content-between small">
                        <div class="text-muted">Copyright &copy; Viwolf System 2020</div>
                        <div>
                            <a href="#">Privacy Policy</a>
                            &middot;
                            <a href="#">Terms &amp; Conditions</a>
                        </div>
                    </div>
                </div>
            </footer>
        </div>
    </div>

 
    @Scripts.Render("~/bundles/ViwolfRentals")
    <script src="~/Areas/Viwolf/Content/js/src/ViwolfRental/ViwolfRental.js"></script>


</body>
</html>
