<body>

    <div class="page-wrapper bg-gra-03 p-t-45 p-b-50">
        <div class="wrapper wrapper--w790">
            <div class="card card-5">
                <div class="card-heading">
                    <h2 class="title">Creación de Unidades</h2>
                </div>
                <div class="card-body">
                    <form id="formCreacionVehiculo" name="formCreacionVehiculo" class="smart-form">
                        <fieldset>
                            <div class="row">
                                <div class="col col-12 col-md-12">
                                   
                                       
                                            <div class="form-row m-b-55">
                                                <div class="col-3">
                                                    <div class="input-group-desc">
                                                        <input id="txtPlacaVehiculo" class="input--style-5" type="text" name="PlacaVehiculo">
                                                        <label class="label--desc">Placa</label>
                                                    </div>
                                                </div>
                                                <div class="col-3">
                                                    <div class="input-group-desc">
                                                        <input id="txtMarcaVehiculo" class="input--style-5" type="text" name="MarcaVehiculo">
                                                        <label class="label--desc">Marca</label>
                                                    </div>
                                                </div>
                                                <div class="col-3">
                                                    <div class="input-group-desc">
                                                        <input id="txtModeloVehiculo" class="input--style-5" type="text" name="ModeloVehiculo">
                                                        <label class="label--desc">Modelo</label>
                                                    </div>
                                                </div>
                                                <div class="col-3">
                                                    <div class="input-group-desc">
                                                        <input id="txtAnnoVehiculo" class="input--style-5" type="text" name="AnnoVehiculo">
                                                        <label class="label--desc">Año</label>
                                                    </div>
                                                </div>
                                            </div>
                                            <div class="form-row m-b-55">
                                                <div class="col-3">
                                                    <div class="input-group-desc">
                                                        <select id="txtGps" class="select--style-5" name="AplicaGps">
                                                            <option>Si</option>
                                                            <option>No</option>
                                                        </select>
                                                        <label class="label--desc">GPS</label>
                                                    </div>
                                                </div>
                                                <div class="col-3">
                                                    <div class="input-group-desc">
                                                        <input id="txtFechaCompra" class="input--style-5" type="text" name="FechaCompra">
                                                        <label class="label--desc">Fecha Compra</label>
                                                    </div>
                                                </div>
                                                <div class="col-3">
                                                    <div class="input-group-desc">
                                                        <input id="txtNumeroChasis" class="input--style-5" type="text" name="NumeroChasis">
                                                        <label class="label--desc">Numero Chasis</label>
                                                    </div>
                                                </div>
                                                <div class="col-3">
                                                    <div class="input-group-desc">
                                                        <input id="txtNumeroMotor" class="input--style-5" type="text" name="NumeroMotor">
                                                        <label class="label--desc">Numero Motor</label>
                                                    </div>
                                                </div>
                                            </div>
                                            <div class="form-row m-b-55">
                                                <div class="col-3">
