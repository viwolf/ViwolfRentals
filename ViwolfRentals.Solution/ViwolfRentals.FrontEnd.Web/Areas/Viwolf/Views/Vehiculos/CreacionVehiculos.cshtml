
@{
    ViewBag.Title = "CreacionVehiculos";
    Layout = "~/Areas/Viwolf/Views/Shared/_LayoutReservacion.cshtml";
}

    <head>
        <!-- Required meta tags-->
        <meta charset="UTF-8">
        <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
        <meta name="description" content="Colorlib Templates">
        <meta name="author" content="Colorlib">
        <meta name="keywords" content="Colorlib Templates">

        <!-- Title Page-->
        <title>@ViewBag.Title - Creación de Unidades</title>

        @Styles.Render("~/bundles/Estilo/ViwolfRentalsReservaciones")

        <!-- Estilo datepicker-->
        <link href="https://code.jquery.com/ui/1.10.4/themes/ui-lightness/jquery-ui.css" rel="stylesheet">

        <!-- Estilo datatable-->
        <link href="https://cdn.datatables.net/responsive/2.1.0/css/responsive.dataTables.min.css" rel="stylesheet" media="screen">
        <link href="https://cdn.datatables.net/1.10.12/css/jquery.dataTables.min.css" rel="stylesheet" media="screen">


        <script type="text/javascript">

        var usuarioLogueado = '@ViewBag.Usuario';
        var idUsuarioLogueado = '@ViewBag.IdUsuario'
        </script>

    </head>



    <body>

        <div class="page-wrapper bg-gra-03 p-t-45 p-b-50">
            <div class="wrapper wrapper--w790">
                <div class="card card-5">
                    <div class="card-heading">
                        <h2 class="title">Creación de Unidades</h2>
                    </div>
                    <div class="card-body">
                        <form id="formCreacionVehiculo" name="formCreacionVehiculo" class="smart-form">
                            <fieldset>
                                <div class="row">
                                    <div class="col col-12 col-md-12">
                                        <div class="form-row m-b-55">
                                            <div class="col-3">
                                                <div class="input-group-desc">
                                                    <input id="txtPlacaVehiculo" class="input--style-5" type="text" name="PlacaVehiculo">
                                                    <label class="label--desc">Placa</label>
                                                </div>
                                            </div>
                                            <div class="col-3">
                                                <div class="input-group-desc">
                                                    <input id="txtMarcaVehiculo" class="input--style-5" type="text" name="MarcaVehiculo">
                                                    <label class="label--desc">Marca</label>
                                                </div>
                                            </div>
                                            <div class="col-3">
                                                <div class="input-group-desc">
                                                    <input id="txtModeloVehiculo" class="input--style-5" type="text" name="ModeloVehiculo">
                                                    <label class="label--desc">Modelo</label>
                                                </div>
                                            </div>
                                            <div class="col-3">
                                                <div class="input-group-desc">
                                                    <input id="txtAnnoVehiculo" class="input--style-5" type="text" name="AnnoVehiculo">
                                                    <label class="label--desc">Año</label>
                                                </div>
                                            </div>
                                        </div>
                                        <div class="form-row m-b-55">
                                            <div class="col-3">
                                                <div class="input-group-desc">
                                                    <select id="txtGps" class="select--style-5" name="AplicaGps">
                                                        <option>Si</option>
                                                        <option>No</option>
                                                    </select>
                                                    <label class="label--desc">GPS</label>
                                                </div>
                                            </div>
                                            <div class="col-3">
                                                <div class="input-group-desc">
                                                    <input id="txtFechaCompra" class="input--style-5" type="text" name="FechaCompra">
                                                    <label class="label--desc">Fecha Compra</label>
                                                </div>
                                            </div>
                                            <div class="col-3">
                                                <div class="input-group-desc">
                                                    <input id="txtNumeroChasis" class="input--style-5" type="text" name="NumeroChasis">
                                                    <label class="label--desc">Numero Chasis</label>
                                                </div>
                                            </div>
                                            <div class="col-3">
                                                <div class="input-group-desc">
                                                    <input id="txtNumeroMotor" class="input--style-5" type="text" name="NumeroMotor">
                                                    <label class="label--desc">Numero Motor</label>
                                                </div>
                                            </div>
                                        </div>
                                        <div class="form-row m-b-55">
                                            <div class="col-3">
                                                <div class="input-group-desc">
                                                    <input id="txtColor" class="input--style-5" type="text" name="Color">
                                                    <label class="label--desc">Color</label>
                                                </div>
                                            </div>
                                            <div class="col-3">
                                                <div class="input-group-desc">
                                                    <input id="txtTransmision" class="input--style-5" type="text" name="Transmision">
                                                    <label class="label--desc">Transmision</label>
                                                </div>
                                            </div>
                                            <div class="col-3">
                                                <div class="input-group-desc">
                                                    <input id="txtCilindraje" class="input--style-5" type="text" name="Cilindraje">
                                                    <label class="label--desc">Cilindrada</label>
                                                </div>
                                            </div>
                                            <div class="col-3">
                                                <div class="input-group-desc">
                                                    <input id="txtPeso" class="input--style-5" type="text" name="Peso">
                                                    <label class="label--desc">Peso</label>
                                                </div>
                                            </div>
                                        </div>
                                        <div class="form-row m-b-55">
                                            <div class="col-3">
                                                <div class="input-group-desc">
                                                    <input id="txtCarroceria" class="input--style-5" type="text" name="Carroceria">
                                                    <label class="label--desc">Carroceria</label>
                                                </div>
                                            </div>
                                            <div class="col-3">
                                                <div class="input-group-desc">
                                                    <input id="txtTraccion" class="input--style-5" type="text" name="Traccion">
                                                    <label class="label--desc">Traccion</label>
                                                </div>
                                            </div>
                                            <div class="col-3">
                                                <div class="input-group-desc">
                                                    <input id="txtCapacidad" class="input--style-5" type="text" name="Capacidad">
                                                    <label class="label--desc">Capacidad</label>
                                                </div>
                                            </div>
                                            <div class="col-3">
                                                <div class="input-group-desc">
                                                    <select id="txtCategoria" class="select--style-5">
                                                        <option value="">Seleccione categoria</option>
                                                    </select>
                                                    <label class="label--desc">Categoría</label>
                                                </div>
                                            </div>
                                        </div>


                                        <div class="form-row m-b-55">
                                            <div class="col-3">
                                                <div class="input-group-desc">
                                                    <input id="txtRtvVencimientoAnno" class="input--style-5" type="text" name="RtvVencimientoAnno">
                                                    <label class="label--desc">Año Vencimiento RTV</label>
                                                </div>
                                            </div>
                                            <div class="col-3">
                                                <div class="input-group-desc">
                                                    <input id="txtRtvVencimientoMes" class="input--style-5" type="text" name="RtvVencimientoMes">
                                                    <label class="label--desc">Mes Vencimiento RTV</label>
                                                </div>
                                            </div>
                                            <div class="col-3">
                                                <div class="input-group-desc">
                                                    <select id="txtRtvSticker" class="select--style-5" name="RtvSticker">
                                                        <option>Si</option>
                                                        <option>No</option>
                                                    </select>
                                                    <label class="label--desc">Sticker RTV</label>
                                                </div>
                                            </div>
                                            <div class="col-3">
                                                <div class="input-group-desc">
                                                    <select id="txtRtvPapel" class="select--style-5" name="RtvPapel">
                                                        <option>Si</option>
                                                        <option>No</option>
                                                    </select>
                                                    <label class="label--desc">Papel RTV</label>
                                                </div>
                                            </div>
                                        </div>
                                        <div class="form-row m-b-55">
                                            <div class="col-3">
                                                <div class="input-group-desc">
                                                    <input id="txtMarchamoProximo" class="input--style-5" type="text" name="MarchamoProximo">
                                                    <label class="label--desc">Próximo Marchamo</label>
                                                </div>
                                            </div>
                                            <div class="col-3">
                                                <div class="input-group-desc">
                                                    <select id="txtMarchamoSticker" class="select--style-5" name="MarchamoSticker">
                                                        <option>Si</option>
                                                        <option>No</option>
                                                    </select>
                                                    <label class="label--desc">Sticker Marchamo</label>
                                                </div>
                                            </div>
                                            <div class="col-3">
                                                <div class="input-group-desc">
                                                    <select id="txtMarchamoPapel" class="select--style-5" name="MarchamoPapel">
                                                        <option>Si</option>
                                                        <option>No</option>
                                                    </select>
                                                    <label class="label--desc">Papel Marchamo</label>
                                                </div>
                                            </div>
                                            <div class="col-3">
                                                <div class="input-group-desc">
                                                    <select id="txtPlacaSticker" class="select--style-5" name="PlacaSticker">
                                                        <option>Si</option>
                                                        <option>No</option>
                                                    </select>
                                                    <label class="label--desc">Sticker Placa</label>
                                                </div>
                                            </div>
                                        </div>
                                        <div class="form-row m-b-55">
                                            <div class="col-3">
                                                <div class="input-group-desc">
                                                    <select id="txtTituloPropiedad" class="select--style-5" name="TituloPropiedad">
                                                        <option>Si</option>
                                                        <option>No</option>
                                                    </select>
                                                    <label class="label--desc">Título de Propiedad</label>
                                                </div>
                                            </div>
                                            <div class="col-3">
                                                <div class="input-group-desc">
                                                    <input id="txtMultas" class="input--style-5" type="text" name="Multas">
                                                    <label class="label--desc">Multas</label>
                                                </div>
                                            </div>
                                            <div class="col-3">
                                                <div class="input-group-desc">
                                                    <input id="txtKilometraje" class="input--style-5" type="text" name="Kilometraje">
                                                    <label class="label--desc">Kilometraje</label>
                                                </div>
                                            </div>
                                            <div class="col-3">
                                                <div class="input-group-desc">
                                                    <select id="txtCodigoColor" class="select--style-5" name="TituloPropiedad">
                                                        <option value="#E5E619">Amarillo</option>
                                                        <option value="#3B83BD">Azul</option>
                                                        <option value="#00AAE4">Celeste</option>
                                                        <option value="#9C9C9C">Gris</option>
                                                        <option value="#572364">Morado</option>
                                                        <option value="#FF5733">Naranja</option>
                                                        <option value="#DF013A">Rojo</option>
                                                        <option value="#31B404">Verde</option>

                                                    </select>
                                                    <label class="label--desc">Clasificacion</label>
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                    <div class="col-md-12">
                                        <div class=" actions pull-right">
                                            <button type="button" id="btnGuardarVehiculo" class="btn btn-accion btn--radius-2 btn-primary btn-sm" data-role="tooltip" title="Crear reservacion">
                                                <i class="fa fa-check-circle display-block fa-2x"></i> Crear Vehiculo
                                            </button>
                                        </div>
                                    </div>
                                </div>
                            </fieldset>
                        </form>
                    </div>
                </div>
            </div>
        </div>

        @Scripts.Render("~/bundles/Content")
        @Scripts.Render("~/bundles/DataTables")

        <!-- Mensajes-->
        <script src="~/Content/js/src/Dialog.js"></script>
        <script src="https://cdnjs.cloudflare.com/ajax/libs/popper.js/1.14.7/umd/popper.min.js" integrity="sha384-UO2eT0CpHqdSJQ6hJty5KVphtPhzWj9WO1clHTMGa3JDZwrnQq4sF86dIHNDz0W1" crossorigin="anonymous"></script>
        <script src="https://stackpath.bootstrapcdn.com/bootstrap/4.3.1/js/bootstrap.min.js" integrity="sha384-JjSmVgyd0p3pXB1rRibZUAYoIIy6OrQ6VrjIEaFf/nJGzIxFDsf4x0xIM+B07jRM" crossorigin="anonymous"></script>


        <script src="~/Areas/Viwolf/Content/js/src/Vehiculos/CreacionVehiculos.js"></script>


    </body><!-- This templates was made by Colorlib (https://colorlib.com) -->
