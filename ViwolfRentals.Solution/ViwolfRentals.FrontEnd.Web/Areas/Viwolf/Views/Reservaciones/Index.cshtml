@{
    ViewBag.Title = "Index";
    Layout = "~/Areas/Viwolf/Views/Shared/_LayoutReservacion.cshtml";
}

    <head>
        <!-- Required meta tags-->
        <meta charset="UTF-8">
        <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
        <meta name="description" content="Colorlib Templates">
        <meta name="author" content="Colorlib">
        <meta name="keywords" content="Colorlib Templates">

        <!-- Title Page-->
        <title>@ViewBag.Title - Creacion Reservaciones</title>

        <!-- Icons font CSS-->
        <!-- Icons font CSS-->
        <link href="~/Areas/Viwolf/Content/js/src/Reservaciones/Content/vendor/mdi-font/css/material-design-iconic-font.min.css" rel="stylesheet" media="all">
        <link href="~/Areas/Viwolf/Content/js/src/Reservaciones/Content/vendor/font-awesome-4.7/css/font-awesome.min.css" rel="stylesheet" media="all">
        <!-- Font special for pages-->
        <link href="https://fonts.googleapis.com/css?family=Open+Sans:300,300i,400,400i,600,600i,700,700i,800,800i" rel="stylesheet">

        <!-- Vendor CSS-->
        <link href="~/Areas/Viwolf/Content/js/src/Reservaciones/Content/vendor/select2/select2.min.css" rel="stylesheet" media="all">
        <link href="~/Areas/Viwolf/Content/js/src/Reservaciones/Content/vendor/datepicker/daterangepicker.css" rel="stylesheet" media="all">

        <!-- Main CSS-->
        <link href="~/Areas/Viwolf/Content/js/src/Reservaciones/Content/css/main.css" rel="stylesheet" media="all">

        <!-- TimePicker CSS-->
        <link rel="stylesheet" href="//cdnjs.cloudflare.com/ajax/libs/timepicker/1.3.5/jquery.timepicker.min.css">
        <link href="https://code.jquery.com/ui/1.10.4/themes/ui-lightness/jquery-ui.css" rel="stylesheet">



        <script type="text/javascript">

        var usuarioLogueado = '@ViewBag.Usuario';
        var idUsuarioLogueado = '@ViewBag.IdUsuario'
        </script>

    </head>

    <body>

        <div class="page-wrapper bg-gra-03 p-t-45 p-b-50">
            <div class="wrapper wrapper--w790">
                <div class="card card-5">
                    <div class="card-heading">
                        <h2 class="title">Creacion de Reservaciones</h2>
                    </div>
                    <div class="card-body">
                        <div id="frmReservacion">
                            <div class="form-row m-b-55">
                                <div class="name">Datos del cliente</div>
                                <div class="value">
                                    <div class="row row-refine">
                                        <div class="col-3">
                                            <div class="input-group-desc">
                                                <input id="txtNombreCliente" class="input--style-5" type="text" name="NombreCliente">
                                                <label class="label--desc">Nombre Cliente</label>
                                            </div>
                                        </div>
                                        <div class="col-3">
                                            <div class="input-group-desc">
                                                <input id="txtHospedaje" class="input--style-5" type="text" name="Hospedaje">
                                                <label class="label--desc">Hospedaje</label>
                                            </div>
                                        </div>
                                        <div class="col-3">
                                            <div class="input-group-desc">
                                                <select id="txtAplicaComision" class="select--style-5" name="AplicaComision">
                                                    <option value=""></option>
                                                    <option value="1">Si</option>
                                                    <option value="0">No</option>
                                                </select>
                                                <label class="label--desc">Aplica Comisión</label>
                                            </div>
                                        </div>
                                        <div class="col-3">
                                            <div class="input-group-desc">

                                                <select id="txtCajon" class="select--style-5" name="Cajon">
                                                    <option value=""></option>
                                                    <option>Si</option>
                                                    <option>No</option>
                                                </select>
                                                <label class="label--desc">Cajon</label>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>

                            @*<div class="form-row m-b-55">
                                <div class="name">Extras</div>
                                <div class="value">
                                    <div class="row row-refine">


                                    </div>
                                </div>
                            </div>*@

                            <div class="form-row m-b-55">
                                <div class="name">Tiempos</div>
                                <div class="value">
                                    <div class="row row-refine">
                                        <div class="col-3">
                                            <div class="input-group-desc">
                                                <input id="txtFechaInicio" class="input--style-5" type="text" name="txtFechaInicio">
                                                <label class="label--desc">Fecha Inicio</label>
                                            </div>
                                        </div>
                                        <div class="col-3">
                                            <div class="input-group-desc">
                                                <input id="txtHoraInicio" class="input--style-5" type="text" name="txtHoraInicio">
                                                <label class="label--desc">Hora Inicio</label>
                                            </div>
                                        </div>
                                        <div class="col-3">
                                            <div class="input-group-desc">
                                                <input id="txtFechaFinal" class="input--style-5" type="text" name="txtFechaFinal">
                                                <label class="label--desc">Fecha Entrega</label>
                                            </div>
                                        </div>
                                        <div class="col-3">
                                            <div class="input-group-desc">
                                                <input id="txtHoraEntrega" class="input--style-5" type="text" name="txtHoraEntrega">
                                                <label class="label--desc">Hora Entrega</label>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>

                            <div class="form-row m-b-55">
                                <div class="name">Montos</div>
                                <div class="value">
                                    <div class="row row-refine">
                                        <div class="col-3">
                                            <div class="input-group-desc">
                                                <input id="txtMontoDia" class="input--style-5" type="text" name="MontoDia">
                                                <label class="label--desc">Monto Día</label>
                                            </div>
                                        </div>
                                        <div class="col-3">
                                            <div class="input-group-desc">

                                                <select id="txtSurfRacks" class="select--style-5" name="SurfRacks">
                                                    <option value=""></option>
                                                    <option>Si</option>
                                                    <option>No</option>
                                                </select>
                                                <label class="label--desc">Surf Racks</label>
                                            </div>
                                        </div>
                                        <div class="col-3">
                                            <div class="input-group-desc">
                                                <input id="txtMontoSurfRacks" disabled class="input--style-5" type="text" name="MontoSurfRacks">
                                                <label class="label--desc">Monto SurfRacks</label>
                                            </div>
                                        </div>
                                        <div class="col-3">
                                            <div class="input-group-desc">
                                                <input id="txtMontoTotal" disabled class="input--style-5" type="text" name="MontoTotal">
                                                <label class="label--desc">Monto Total</label>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>



                            <div class="form-row m-b-55">
                                <div class="name">Depósitos</div>
                                <div class="value">
                                    <div class="row row-refine">
                                        <div class="col-3">
                                            <div class="input-group-desc">
                                                <select id="txtAplicaPagoAdelantado" class="select--style-5" name="AplicaPagoAdelantado">
                                                    <option value=""></option>
                                                    <option value="1">Si</option>
                                                    <option value="0">No</option>
                                                </select>
                                                <label class="label--desc">Pago Adelantado</label>

                                                @*<input id="txtUsuario" disabled class="input--style-5" type="text" name="Usuario">
                                                <label class="label--desc">Reservado por</label>*@
                                            </div>
                                        </div>
                                        <div class="col-3">
                                            <div class="input-group-desc">
                                                <input id="txtNumeroDeposito" disabled class="input--style-5" type="text" name="NumeroDeposito">
                                                <label class="label--desc">Número Depósito</label>
                                            </div>
                                        </div>
                                        <div class="col-3">
                                            <div class="input-group-desc">
                                                <input id="txtMontoDeposito" disabled class="input--style-5" type="text" name="MontoDepósito">
                                                <label class="label--desc">Monto Depósito</label>
                                            </div>
                                        </div>
                                        <div class="col-3">
                                            <div class="input-group-desc">
                                                <input id="txtSaldoActual" disabled class="input--style-5" type="text" name="SaldoActual">
                                                <label class="label--desc">Saldo Actual</label>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>






                            <div class="form-row m-b-55">
                                <div class="name">Datos</div>
                                <div class="value">
                                    <div class="row row-refine">

                                        <div class="col-3">
                                            <div class="input-group-desc">

                                                <select id="txtModoPago" disabled class="select--style-5" name="ModoPago">
                                                    <option value="0">Seleccione pago</option>
                                                    <option value="1">Efectivo</option>
                                                    <option value="2">Cuenta por Cobrar</option>
                                                </select>
                                                <label class="label--desc">Modo de Pago</label>
                                            </div>
                                        </div>
                                        <div class="col-3">
                                            <div class="input-group-desc">

                                                <select id="txtComisionistas" disabled class="select--style-5">
                                                    <option value="">Seleccione comisionista</option>
                                                </select>
                                                <label class="label--desc">Comisionistas</label>
                                            </div>
                                        </div>
                                        <div class="col-3">
                                            <div class="input-group-desc">
                                                <select id="txtProveedor" disabled class="select--style-5">
                                                    <option value="">Seleccione proveedor</option>
                                                </select>
                                                <label class="label--desc">Proveedores</label>
                                            </div>
                                        </div>
                                        <div class="col-3">
                                            <div class="input-group-desc">
                                                <input id="txtPlaca" disabled class="input--style-5" type="text" name="Placa">
                                                <label class="label--desc">Número de Placa</label>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>


                            <div class="col-md-12">
                                <div class=" actions pull-right">
                                    <button type="button" id="btnGuardar" class="btn btn-accion btn--radius-2 btn-primary btn-sm" data-role="tooltip" title="Crear reservacion">
                                        <i class="fa fa-check-circle display-block fa-2x"></i> Crear Reservacion
                                    </button>
                                    <button type="button" id="btnCargarVehiculo" class="btn btn--radius-2 btn-info" data-role="tooltip" title="Informacion del vehiculo">
                                        <i class="fa fa-info-circle display-block fa-2x"></i> Mostrar Vehiculo
                                    </button>
                                </div>
                            </div>


                            @*<div>
                                <button id="btnGuardar" class="btn btn-accion btn--radius-2 btn-primary btn-sm" type="submit">Crear Reservacion</button>
                                <button id="btnInfo" disabled class="btn btn--radius-2 btn-info" type="submit">Mostrar Vehiculo</button>
                            </div>*@
                        </div>
                    </div>
                </div>
            </div>
        </div>

        <!-- Jquery JS-->
        <script src="https://code.jquery.com/jquery-1.12.4.js"></script>
        <script src="~/Areas/Viwolf/Content/js/src/Reservaciones/Content/js/jquery-ui.js"></script>

        <!-- Vendor JS-->
        <script src="~/Areas/Viwolf/Content/js/src/Reservaciones/Content/vendor/select2/select2.min.js"></script>
        <script src="~/Areas/Viwolf/Content/js/src/Reservaciones/Content/vendor/datepicker/moment.min.js"></script>
        <script src="~/Areas/Viwolf/Content/js/src/Reservaciones/Content/vendor/datepicker/daterangepicker.js"></script>


        <!-- Main JS-->

        <script src="~/Content/Plugins/ajaxloader/ajaxloader.js"></script>
        <script src="~/Areas/Seguridad/Content/js/src/app.js"></script>

        <!-- Mensajes-->
        <script src="~/Content/js/src/Dialog.js"></script>
        <script src="https://cdnjs.cloudflare.com/ajax/libs/popper.js/1.14.7/umd/popper.min.js" integrity="sha384-UO2eT0CpHqdSJQ6hJty5KVphtPhzWj9WO1clHTMGa3JDZwrnQq4sF86dIHNDz0W1" crossorigin="anonymous"></script>
        <script src="https://stackpath.bootstrapcdn.com/bootstrap/4.3.1/js/bootstrap.min.js" integrity="sha384-JjSmVgyd0p3pXB1rRibZUAYoIIy6OrQ6VrjIEaFf/nJGzIxFDsf4x0xIM+B07jRM" crossorigin="anonymous"></script>


        <script src="~/Areas/Viwolf/Content/js/src/Reservaciones/Reservaciones.js"></script>

        @*Time Picker*@
        <script src="//cdnjs.cloudflare.com/ajax/libs/timepicker/1.3.5/jquery.timepicker.min.js"></script>


        <script src="~/Content/js/src/utils.js"></script>

        <!--Informacion del Vehiculo-->
        @Html.Partial("~/Areas/Viwolf/Views/Vehiculos/VehiculoBuscar.cshtml")

        <script src="~/Areas/Viwolf/Content/js/src/Vehiculos/BuscarVehiculo.js"></script>

    </body>