
@{
    ViewBag.Title = "Index";
    Layout = "~/Areas/Viwolf/Views/Shared/_LayoutReservacion.cshtml";
}


<!DOCTYPE html>

<head>
    <!-- Required meta tags-->
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
    <meta name="description" content="Colorlib Templates">
    <meta name="author" content="Colorlib">
    <meta name="keywords" content="Colorlib Templates">

    <!-- Title Page-->
    <title>@ViewBag.Title - Control de Contratos</title>
    @Styles.Render("~/bundles/Estilo/ViwolfRentalsReservaciones")

    <!-- Estilo datepicker-->
    <link href="https://code.jquery.com/ui/1.10.4/themes/ui-lightness/jquery-ui.css" rel="stylesheet">

    <!-- Estilo datatable-->
    <link href="https://cdn.datatables.net/responsive/2.1.0/css/responsive.dataTables.min.css" rel="stylesheet" media="screen">
    <link href="https://cdn.datatables.net/1.10.12/css/jquery.dataTables.min.css" rel="stylesheet" media="screen">

    <!-- TimePicker CSS-->
    <link rel="stylesheet" href="//cdnjs.cloudflare.com/ajax/libs/timepicker/1.3.5/jquery.timepicker.min.css">
    <link href="https://code.jquery.com/ui/1.10.4/themes/ui-lightness/jquery-ui.css" rel="stylesheet">

    <script type="text/javascript">
        var usuarioLogueado = '@ViewBag.Usuario';
        var idUsuarioLogueado = '@ViewBag.IdUsuario'
    </script>
</head>

<body>
    <div class="page-wrapper bg-gra-03 p-t-45 p-b-50">
        <div class="wrapper wrapper--w790">
            <div class="card card-5">
                <div class="card-heading">
                    <h2 class="title">Control de Contratos</h2>
                </div>
                <div class="card-body">
                    @*<form id="formPagoComision" name="formPagoComision" class="smart-form">*@
                    <fieldset>
                        <div class="row">
                            <div class="col col-12 col-md-12">
                                <div class="form-row m-b-55">
                                    <div class="col-4">
                                        <input id="txtNumeroContrato" class="input--style-5" type="text" name="NumeroContrato">
                                        <label class="label--desc">Numero Contrato</label>
                                    </div>
                                    <div class="col-8">
                                        <input id="txtNombreCliente" class="input--style-5" type="text" name="NombreCliente">
                                        <label class="label--desc">Nombre Cliente</label>
                                    </div>
                                </div>
                            </div>
                        </div>
                        <div class="row">
                            <div class="col col-12 col-md-12">
                                <div id="divDataComisiones" class="col-md-12" style="overflow-x: auto">
                                    <table id="tblDataContratos" class="table table-striped table-hover dt-responsive" cellspacing="0" width="100%">
                                        <thead>
                                            <tr>
                                                <th style="visibility:hidden">IDContrato</th>
                                                <th style="visibility:hidden">Reservaciones</th>
                                                <th style="visibility:hidden">IDEstadoContrato</th>
                                                <th>Numero Contrato </th>
                                                <th>Nombre del cliente</th>
                                                <th>Fecha Inicio</th>
                                                <th>Fecha Regreso</th>
                                                <th>Estado</th>
                                                <th></th>
                                               
                                            </tr>
                                        </thead>
                                    </table>
                                </div>
                            </div>
                        </div>
                        <div class="row">
                            <div class="col-md-12">
                                <div class=" actions pull-right">
                                    <button type="button" id="btnBuscarContrato" class="btn btn-accion btn--radius-2 btn-primary btn-sm" data-role="tooltip" title="Buscar Reservacion">
                                        <i class="fa fa-search"></i> Buscar
                                    </button>

                                </div>
                            </div>
                        </div>
                    </fieldset>
                    @*</form>*@
                </div>
            </div>
        </div>
    </div>
</body>
@Scripts.Render("~/bundles/Content")
@Scripts.Render("~/bundles/DataTables")



<!-- Mensajes-->
<script src="~/Content/js/src/Dialog.js"></script>
<script src="https://cdnjs.cloudflare.com/ajax/libs/popper.js/1.14.7/umd/popper.min.js" integrity="sha384-UO2eT0CpHqdSJQ6hJty5KVphtPhzWj9WO1clHTMGa3JDZwrnQq4sF86dIHNDz0W1" crossorigin="anonymous"></script>
<script src="https://stackpath.bootstrapcdn.com/bootstrap/4.3.1/js/bootstrap.min.js" integrity="sha384-JjSmVgyd0p3pXB1rRibZUAYoIIy6OrQ6VrjIEaFf/nJGzIxFDsf4x0xIM+B07jRM" crossorigin="anonymous"></script>

@*Time Picker*@
<script src="//cdnjs.cloudflare.com/ajax/libs/timepicker/1.3.5/jquery.timepicker.min.js"></script>

<script src="~/Areas/Viwolf/Content/js/src/Contratos/ControlContratos.js"></script>

@Html.Partial("~/Areas/Viwolf/Views/Contratos/ExtenderContrato.cshtml")
<script src="~/Areas/Viwolf/Content/js/src/Contratos/ExtenderContrato.js"></script>

<!--Informacion del Vehiculo-->
@Html.Partial("~/Areas/Viwolf/Views/Vehiculos/VehiculoBuscar.cshtml")
<script src="~/Areas/Viwolf/Content/js/src/Vehiculos/BuscarVehiculo.js"></script>